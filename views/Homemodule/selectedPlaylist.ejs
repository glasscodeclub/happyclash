                    </svg>                    
            </div>
            <div class="share-no">
                <p>8</p>
            </div>
        </div>
    </div>

    <div class="similar-playlist">
        <!-- POPUP SHOWING MORE SONGS IN PLAYLIST -->
     
            <div class="popup-playlist active" id="popup">
                <div class="close-btn" data-close-btn >&times;</div>
                <h6>More videos in this playlist:</h6>
                <% for(var i=1; i<=5; i++){ %>
                    <div class=" challenge-songs">
                        <div class="  challenge-name">
                            <div class="song-name">
                                song challenge
                            </div>
                        </div>
                        <div class=" challenge-details ">
                            <div class="challenge-title">song challenge video #<%= i %></div>
                            <div class="votes">votes: 350</div>
                            <div class="comments-count">comments: 113</div>
                        </div>
                    </div>
                    <hr class="hr-inside-popup">
                <% } %>    
        
            </div>
        <!-- POPUP END -->
        <hr class="hr-pop active" id="hr-pop">
        <h3>Similar Playlist: </h3>
        <!-- SIMILAR PLAYLIST -->
       
            <% for(var i=0; i<5; i++){ %>
                <div class="similar-playlist-table">
                    <div class="similar-song">
                        <p class="song-name simi-song">Song name</p>
                    </div>
                    <div class="similar-details">
                        <div class="creator-name">created by: jenne wilson</div>
                        <div class="created-on">created on: 10 May 2021</div>
                        <div class="participants">Participants: 39</div>
                        <div class="clash-till">Clash till: 10 june 2021</div>
                    </div>
                </div>
                <hr class="hr-outside">
            <%}%>
        <!-- </div> -->
    </div>
    <div class="bottom">
        <%- include("../BottomNavmodule/bottomnavbar") %>
    </div>
</body>
