                <defs>
                <linearGradient id="paint0_linear" x1="3.00056" y1="0.36908" x2="3.00056" y2="32.375" gradientUnits="userSpaceOnUse">
                <stop stop-color="#FEE16B"/>
                <stop offset="0.53125" stop-color="#E660B8"/>
                <stop offset="1" stop-color="#7749F8"/>
                </linearGradient>
                <linearGradient id="paint1_linear" x1="15.9504" y1="0.369072" x2="15.9504" y2="32.375" gradientUnits="userSpaceOnUse">
                <stop stop-color="#FEE16B"/>
                <stop offset="0.53125" stop-color="#E660B8"/>
                <stop offset="1" stop-color="#7749F8"/>
                </linearGradient>
                </defs>
                </svg>
                
        </div>

        <div class="play controls">
            <img src="/img/Play.svg" alt="">
                
        </div>

        <div class="comments controls" onclick="comment()">
            <div class="comment-btn">
                <svg width="23" height="20" viewBox="0 0 23 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path d="M0.497437 0V12.5H3.62244V16.4797L4.89197 15.4539L8.57869 12.5H16.1224V0H0.497437ZM2.05994 1.5625H14.5599V10.9375H8.04119L7.82166 11.1086L5.18494 13.2078V10.9375H2.05994V1.5625ZM17.6849 3.125V4.6875H20.8099V14.0625H17.6849V16.3328L14.8287 14.0625H8.96931L7.01619 15.625H14.2912L19.2474 19.6047V15.625H22.3724V3.125H17.6849Z" fill="black"/>
                    </svg>                    
            </div>
            <div class="comment-no">
                <p>23</p>
            </div>
        </div>

        <div class="share controls" onclick="share()">
            <div class="share-btn">
                <svg width="22" height="22" viewBox="0 0 22 22" fill="none" xmlns="http://www.w3.org/2000/svg">
