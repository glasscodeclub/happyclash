<!doctype html>
<html lang="en">
  <head>
  	<title>Profile page</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    
        <link rel="stylesheet" href="/css/profile.css">
        <link rel="stylesheet" href="/css/style_hh.css">
        <!-- <link rel="stylesheet" href="css/style.css"> -->
		<link rel="stylesheet" href="/css/bottomnavbar.css"> 
		<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-BmbxuPwQa2lc/FVzBcNJ7UAyJxM6wuqIj61tLrc4wSX0szH/Ev+nYRRuWlolflfl" crossorigin="anonymous">
		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/js/bootstrap.bundle.min.js" integrity="sha384-b5kHyXgcpbZJO/tY9Ul7kGkf1S0CWuKcCD38l8YkeH8z8QjE0GmW1gYU5S9FOnJ0" crossorigin="anonymous"></script>
  </head>
  <body>
		
    <div class="container" style="margin-top: 30px; margin-bottom: 5px;">
        <div class="row">
            <div class="col-3">
            </div>
            <div class="col-6">
                <img src="img/profile.png" class="p1">
                <h6 class="n1">Jane Cooper</h6>
                <p id="n2">New York, USA </p>
            </div>
            <div class="col-3">
            </div>
          </div>
    </div>
    <div class="container" style="margin-top: 10px; margin-bottom: 5px;">
        <div class="row">
            <div class="col-1">
            </div>
            <div class="col-10">
                <button type="button" class="btn btn-light br1">Career</button>
                <button type="button" class="btn btn-light br1">Follow</button>
                <button type="button" class="btn btn-light  br1">Edit Profile</button>
            </div>
            <div class="col-1">
            </div>
          </div>
    </div>
    <div class="container" style="margin-top: 30px; margin-bottom: 5px;">
        <div class="row">
            <div class="col-3">
                <h6 class="rp2">54</h6>
                <p class="rp1">Followers</p>
            </div>
            <div class="col-3">
                <h6 class="rp2">34</h6>
                <p class="rp1">Following</p>
            </div>
            <div class="col-3">
                <h6 class="rp2">18</h6>
                <p class="rp1">Videos</p>
            </div>
            <div class="col-3">
                <h6 class="rp2">3</h6>
                <p class="rp1">Clash Won</p>
            </div>
          </div>
    </div>
    <div class="container" style="margin-top: 20px; margin-bottom: 5px;">
        <div class="row">
            <div class="col-3">
                <h4>Posts</h4>
            </div>
            <div class="col-6"></div>
            <div class="col-3">
                <p>See All</p>
            </div>
        </div>
    </div>

    <div id="carouselExampleDark" class="carousel carousel-dark slide mt" data-bs-ride="carousel">
        <div class="carousel-inner">
          <div class="carousel-item active" data-bs-interval="36000000">
            <video controls width="310" class="vid">
                <source src="videos/vid1.mp4"type="video/webm">
            </video>
          </div>
          <div class="carousel-item" data-bs-interval="36000000">
            <video controls width="310" class="vid">
                <source src="videos/vid2.mov"type="video/webm">
            </video>
          </div>
          <div class="carousel-item">
            <video controls width="310" class="vid" data-bs-interval="36000000">
                <source src="videos/vid3.mov"type="video/webm">
            </video>
          </div>
        </div>
    </div>
		<%- include("../partials/bottomnavbar") %>

  </body>
</html>
